<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.practice.basic.repository.MainMapper">

    <insert id="insertMember" parameterType="com.practice.basic.dto.MemberForm">
        insert into
        member(email, password, name)
        values(#{email}, #{password}, #{name})
    </insert>

    <select id="getMemberList" resultType="com.practice.basic.dto.MemberDto">
        SELECT
        *
        FROM member
    </select>

</mapper>
